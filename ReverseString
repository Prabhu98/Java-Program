/* Psuedocode

a) Add an empty string assign an variable.
b) Iterate for loop at the end_Index-1 to Start_Index
c) Add each character to the empty string till for loop ends
d) Print the value of the result
*/
 
 
package DemoProject.APIdemo;

import java.util.ArrayList;
import java.util.Arrays;

public class RemoveElement {  
    
    public static void main(String[] args) {
    String st = "Welcome";

		String rev ="";
		 
		 for(int i=st.length()-1;i>=0;i--) {
		 
		 rev = rev + st.charAt(i); 
		 
		 }
		 
		 System.out.println(rev);
     }
}
